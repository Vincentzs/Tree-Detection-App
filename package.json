{
    "name": "korotu-backend",
    "version": "1.0.0",
    "description": "The backend application for the korotu tree detection app",
    "main": "index.ts",
    "scripts": {
        "dev": "nodemon --exec babel-node src/index.ts --extensions \".ts\" ",
        "clean-nuke": "rm -rf node_modules",
        "clean-install": "npm ci",
        "clean-build": "npm run clean-install && npm run build",
        "build": "babel src --out-dir dist --extensions .ts,.js",
        "start": "npm run build && node dist/index.js",
        "test": "jest",
        "deploy": "heroku config:set NPM_CONFIG_PRODUCTION=false && heroku buildpacks:set heroku/nodejs && git push heroku main"
    },
    "engines": {
        "node": ">=18.x"
    },
    "moduleDirectories": [
        "node_modules",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/csc301-2023-winter/13-Korotu-T.git"
    },
    "author": "CSC301 Team 13",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/csc301-2023-winter/13-Korotu-T/issues"
    },
    "homepage": "https://github.com/csc301-2023-winter/13-Korotu-T#readme",
    "dependencies": {
        "axios": "^1.3.4",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "eslint": "^8.33.0",
        "exif-parser": "^0.1.12",
        "express": "^4.18.2",
        "fuzzysort": "^2.0.4",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "parse": "^4.0.1",
        "sharp": "^0.31.3",
        "supertest": "^6.3.3",
        "uuid": "^7.x"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/node": "^7.20.7",
        "@babel/plugin-transform-modules-commonjs": "^7.21.2",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.11.19",
        "babel-jest": "^29.4.3",
        "babel-plugin-transform-remove-strict-mode": "^0.0.2",
        "concurrently": "^7.6.0",
        "jest": "^29.4.3",
        "nodemon": "^2.0.21",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.5"
    }
}
